include /layouts/base.pug
.toc-section.js-toc
    h3.toc-section__title.js-toc__trigger Content
        +icon("toc-arrow")
    nav.toc-section__nav 
        ol.toc-section__list 
            each item in toc 
                li.toc-section__item 
                    a(href=item.link).toc-section__link= item.title 